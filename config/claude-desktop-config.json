{
  "mcpServers": {
    "agr-genomics-enhanced-js": {
      "command": "node",
      "args": ["<PROJECT_PATH>/src/agr-server-enhanced.js"],
      "cwd": "<PROJECT_PATH>",
      "env": {
        "NODE_ENV": "production",
        "LOG_LEVEL": "info",
        "API_TIMEOUT": "30000",
        "CACHE_TTL": "300",
        "CACHE_MAX_KEYS": "1000"
      }
    },
    "agr-genomics-dev": {
      "command": "node",
      "args": ["--inspect", "<PROJECT_PATH>/src/agr-server-enhanced.js"],
      "cwd": "<PROJECT_PATH>",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "debug",
        "API_TIMEOUT": "60000",
        "CACHE_TTL": "60"
      }
    }
  }
}